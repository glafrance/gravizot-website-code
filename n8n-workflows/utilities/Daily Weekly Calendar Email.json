    {
      "parameters": {
        "jsCode": "// Get current date\nconst curr = new Date();\n\n// Calculate days until next Monday\nconst daysUntilNextMonday = ((8 - curr.getDay()) % 7) || 7;\n\n// Get next Monday at 00:00:00\nconst nextMonday = new Date(curr);\nnextMonday.setDate(curr.getDate() + daysUntilNextMonday);\nnextMonday.setHours(0, 0, 0, 0);\n\n// Get next Friday at 23:59:59\nconst nextFriday = new Date(nextMonday);\nnextFriday.setDate(nextMonday.getDate() + 4);\nnextFriday.setHours(23, 59, 59, 999);\n\n// Set output\nitems[0].json.from = nextMonday.toISOString();\nitems[0].json.to = nextFriday.toISOString();\n\nreturn items;"
      },
